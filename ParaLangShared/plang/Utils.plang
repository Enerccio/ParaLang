import cz.upol.vanusanik.paralang.runtime.wrappers.PLTime;

module Utils {

    class Time {
        var wrappedTime;
        
        defn init(unixTime){
            wrappedTime = PLTime->PLTime(unixTime);
        }
        
        defn get_time(){
            return wrappedTime->getUnixTime();
        }
    }

};