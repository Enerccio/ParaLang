using IO;
using Collections.List;

module HelloWorldModule {
	
	restricted run(){
		var list = new List();
		list.add(1);
		list.add(2);
		list.add(3);
		
		IO.out.println(list);
		IO.out.println(list.size());
		
		var list2 = list.append(4);
		
		list.clear();
		IO.out.println(list);
		IO.out.println(list2);
		IO.out.println(list.size());
	}
};