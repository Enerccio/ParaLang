using SecondModule.Class4;
import java.util.Map;

module HelloWorldModule {
	var foobar;
	
	class Helper {
		var augumented;
		
		init(augumented){
			inst.augumented = augumented;
		}
		
		restricted foo(arg){
			augumented = new Helper(arg);
		}
	}
	
	init(){
		foobar = new Helper(1);
	}
	
	var foobaz;
	
	restricted run(){
		foobaz = new Helper(foobar);
		foobar.augumented = foobaz;
	}
};