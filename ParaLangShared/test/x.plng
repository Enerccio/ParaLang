using IO;
using Collections.List;

module HelloWorldModule {

	class SimpleException > BaseException {
		
		get_message(){
			return "MESSAGE: " + parent.get_message();
		}
	
	} 

	class Test {
		var a, b;
		
		restricted __apply(){
			run();
		}
		
		restricted run(){
			b = 6;
			
			throw new SimpleException("Oh damn! Something went wrong!");
			
			a = new Integer(b);
			IO.out.println(a == b);
			IO.out.println(b == a);
			
			var c = 5;
			var d = 8;
			
			IO.out.println(c > new Integer(a));
			IO.out.println(d < a);
			
			IO.out.println(c + a + b);
			
			inst == new Test();
		}
		
		__eq(me, other){
			IO.out.println("Test!");
			return parent.__eq(me, other);
		}
	}
	
	restricted run(){
		try {
			new Test()();
		} catch (BaseException a){
			IO.out.println("Caught a!");
			IO.out.println(a);
			throw a;
		} finally {
			IO.out.println("Finally done this!");
		} 
	}
};