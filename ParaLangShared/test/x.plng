using SecondModule.Class4;
import java.util.Map;

module HelloWorldModule {
	var foobar;
	
	class Helper {
		var augumented;
		
		init(augumented){
			if (augumented){
				inst.augumented = 10;
			} else
				inst.augumented = 1.5;
			foobar = 1.0;
		}
		
		restricted foo(arg){
			augumented = new Helper(arg);
		}
	}
	
	init(){
		foobar = new Helper(1);
	}
	
	var foobaz;
	
	restricted run(){
		foobaz = new Helper(foobar);
		foobaz.augumented = foobaz;
	}
};