using IO;
using Collections.List;

module HelloWorldModule {

	class Test {
		var list, list2;
		
		restricted run(){
			list = new List();
			list.add(1);
			list.add(2);
			list.add(3);
			list.add(list);
			
			IO.out.println(list);
			IO.out.println(list.size());
			
			list2 = list.append(4);
			
			list.clear();
			IO.out.println(list);
			IO.out.println(list2);
			IO.out.println(list.size());
		}
	}
	
	restricted run(){
		new Test().run();
	}
};