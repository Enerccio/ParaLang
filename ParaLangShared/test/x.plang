using IO;
using Collections.List;

module HelloWorldModule {

	class GaspingException > BaseException {
		
		get_message(){
			return "Oh! " + parent.get_message() + "!!!";
		}
	
	} 

	class Test {
		var a, b;
		
		restricted __apply(){
			run();
		}
		
		restricted run(){
			b = 6;
			
			a = new Integer(b);
			IO.out.println(a == b);
			IO.out.println(b == a);
			
			var c = 5;
			var d = 8;
			
			IO.out.println(c > new Integer(a));
			IO.out.println(d < a);
			
			IO.out.println(c + a + b);
			
			inst == new Test();
			
			throw new GaspingException("Something went wrong");
		}
		
		__eq(other){
			IO.out.println("Test!");
			return parent.__eq(other);
		}
	}
	
	restricted run(){
		try {
			try {
				return new Test();
			} catch (BaseException a){
				IO.out.println("Caught a!");
				IO.out.println(a);
				throw a;
			} finally {
				IO.out.println("Finally done this!");
			}
		} finally {
			IO.out.println("Outer layer finally");
		} 
	}
};