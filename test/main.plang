using IO;
using Math;
using Math.Matrix;
using Random.Random;
using Random;
using System;

module Main {
    var r = new Random.Random(NoValue);

    restricted calculate(a, b){
        var ct = System.current_time();
    
        for (var i=0; i<10; i++){
            IO.out.println(Random.random.get_clamp());
            IO.out.println(Random.java_random.get_clamp());
            IO.out.println(Random.secure_random.get_clamp());
        }
    
        var m1 = Math.random_matrix(a, a, r, 100);
        var m2 = Math.random_matrix(b, b, r, 50);
        
        IO.out.println(m1);
        IO.out.println(m2);
        
        IO.out.println(m1 * 10);
        IO.out.println(m1 * m2);
        
        IO.out.println(Math.sqrt(4) + Math.sqrt(2.2));
        
        IO.out.println(System.current_time() - ct);
    }
    
};