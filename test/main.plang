using IO;
using Math;
using Math.Matrix;
using Random.Random;
using Random;
using System;

module Main {
    var r = new Random.Random(NoValue);

    defn restricted calculate(a, b){
        var ct = System.current_time();
        
        IO.out.println("Ahoj".to(3).from(1));
    
        var m1 = Math.random_matrix(a, a, r, 100);
        var m2 = Math.random_matrix(b, b, r, 50);
        
        IO.out.println(m1);
        IO.out.println(m2);
        
        IO.out.println(m1 * 10);
        IO.out.println(m1 * m2);
        
        IO.out.println(Math.sqrt(4) + Math.sqrt(2.2));
        
        IO.out.println(System.current_time() - ct);
    }
    
};